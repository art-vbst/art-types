{
  "name": "@art-vbst/art-types",
  "version": "1.0.5",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "stub"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "biome format --write ./src",
    "format:unsafe": "biome format --write --unsafe ./src",
    "lint": "biome lint .",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/art-vbst/art-types.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "typescript": "^5.9.3"
  }
}
